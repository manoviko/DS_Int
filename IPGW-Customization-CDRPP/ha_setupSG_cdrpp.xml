<!DOCTYPE dsu SYSTEM "/usr/cti/conf/swp/networking.dtd">
<swpHA>
	<configuration>
		<group>
			<name>ConverterTool</name>
			<attribute>
				<AutoStart>1</AutoStart>
				<AutoStartPolicy>Order</AutoStartPolicy>
				<FailOverPolicy>Priority</FailOverPolicy>
			</attribute>
			<resource>
				<type>Mount</type>
				<name>asntool-data-mount</name>
				<attribute>
					<NetAppID>__asntool_data_volume_ip__</NetAppID>
					<NetAppVol>__full_path_to_asntool_data__</NetAppVol>
					<MountPoint>__local_cdrs_location__</MountPoint>
					<MountOpt>"rw,bg,hard,nointr,tcp,vers=3,timeo=600,rsize=32768,wsize=32768"</MountOpt>
				  </attribute>
			</resource>
		</group>
	</configuration>
</swpHA>