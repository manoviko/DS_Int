<?xml version="1.0" encoding="utf-8"?>
<configuration>
	<Customized-parameters>
		<database-configure-parameters>
			<ORACLE_CONFIG_TYPE>Single-instance</ORACLE_CONFIG_TYPE>
			<DB_NAME>sem_db</DB_NAME>
			<ORACLE_DATA_DIR>/data/sem_db_data</ORACLE_DATA_DIR>
			<ORACLE_LOG_DIR>/data/sem_db_logs</ORACLE_LOG_DIR>     
			<ORACLE_ARCH_DIR>/data/sem_db_archives</ORACLE_ARCH_DIR> 
			<ORACLE_BACKUP_DIR>/data/sem_db_backup</ORACLE_BACKUP_DIR>   
			<ORACLE_ARCH_RESERVED_SPACE></ORACLE_ARCH_RESERVED_SPACE>   
			<ORACLE_ADDITIONAL_DATA_DIR/>   
			<ARCH_MODE>true</ARCH_MODE>
			<BACKUP_MODE>true</BACKUP_MODE>
			<BACKUP_TYPE>rman</BACKUP_TYPE>
			<ORACLE_BACKUP_MODE>FULL</ORACLE_BACKUP_MODE>
      <ORACLE_BACKUP_LEVEL></ORACLE_BACKUP_LEVEL>
      <ORACLE_BACKUP_COMPRESSION>BASIC</ORACLE_BACKUP_COMPRESSION>
      <ORACLE_BACKUP_PARALLELISM>2</ORACLE_BACKUP_PARALLELISM>
      <ORACLE_BACKUP_FILES_PER_SET>1</ORACLE_BACKUP_FILES_PER_SET>
      <ORACLE_BACKUP_KEEP_TIME>7</ORACLE_BACKUP_KEEP_TIME>
      <ORACLE_BACKUP_SEND_ALERT>yes</ORACLE_BACKUP_SEND_ALERT>
			<ORACLE_MEM>1500</ORACLE_MEM>
			<XDB_ENABLED>false</XDB_ENABLED>
		</database-configure-parameters>
		<cluster-configure-parameters>
			<oracle-cluster>
				<IS1NODE>Yes</IS1NODE>
				<CLUSTER_CRS_GROUP></CLUSTER_CRS_GROUP>
				<CLUSTER_CRS_RESOURCE></CLUSTER_CRS_RESOURCE>	
				<CLUSTER_ORACLE_INFRASTRUCTURE_GROUP/>
				<SCAN_HOSTNAME></SCAN_HOSTNAME>
				<SCAN_IP></SCAN_IP>
			</oracle-cluster>
			<veritas-cluster>	
				<ORACLE_HA_XML_FILE/>
				<CLUSTER_ORACLE_GROUP>sem_db</CLUSTER_ORACLE_GROUP>
				<CLUSTER_ORACLE_RESOURCE>sem_db-oracle</CLUSTER_ORACLE_RESOURCE>
				<CLUSTER_LISTENER_RESOURCE>sem_db-listener</CLUSTER_LISTENER_RESOURCE>
				<CLUSTER_DEPENDENCIES>false</CLUSTER_DEPENDENCIES>
				<CLUSTER_ORACLE_DEPENDENCIES/>
			</veritas-cluster>
		</cluster-configure-parameters>
	</Customized-parameters>
	<Constant-parameters>
			<DB_BLOCK_SIZE>8192</DB_BLOCK_SIZE>
			<IS_MTS>false</IS_MTS>
			<REDO_LOG_GROUPS>5</REDO_LOG_GROUPS>
			<REDO_LOG_SIZE>500</REDO_LOG_SIZE>
			<BACKUP_SCHEDULE_TIME>12</BACKUP_SCHEDULE_TIME> 
			<NUMBER_OF_BACKUP_SETS_ON_DISK>2</NUMBER_OF_BACKUP_SETS_ON_DISK>
			<APPLICATION_SESSIONS>500</APPLICATION_SESSIONS>
			<MIN_ORACLE_MEM_SIZE>800</MIN_ORACLE_MEM_SIZE>
			<PERCENT_OF_RAM_SIZE>0</PERCENT_OF_RAM_SIZE>	
			<MAX_PERCENT_OF_RAM>95</MAX_PERCENT_OF_RAM>						
	</Constant-parameters>
</configuration>